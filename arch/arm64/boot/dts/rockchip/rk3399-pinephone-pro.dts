	compatible = "pine64,pinephone-pro", "rockchip,rk3399";
	chassis-type = "handset";

	aliases {
		mmc0 = &sdio0;
		mmc1 = &sdmmc;
		mmc2 = &sdhci;
	};

	chosen {
		bootargs = "earlycon=uart8250,mmio32,0xff1a0000";
