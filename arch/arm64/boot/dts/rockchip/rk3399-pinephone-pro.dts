		wcam_pdn: wcam-pdn {
			rockchip,pins = <2 RK_PD4 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		ucam_rst: ucam-rst {
			rockchip,pins = <1 RK_PA4 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		ucam_pdn: ucam-pdn {
			rockchip,pins = <2 RK_PB4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	fusb302x {
		fusb0_int: fusb0-int {
